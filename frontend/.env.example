# Frontend example env (rename to .env and set values)

# Backend API base URL
VITE_API_BASE=http://localhost:3000

# OAuth (frontend / login UI)
VITE_OAUTH_CLIENT_ID=
VITE_OAUTH_AUTHORIZE_URL=https://accounts.google.com/o/oauth2/auth
VITE_OAUTH_TOKEN_URL=https://oauth2.googleapis.com/token
VITE_OAUTH_REDIRECT_URI=http://localhost:3000/api/oauth/callback
VITE_OAUTH_SCOPES="openid profile email"
VITE_OAUTH_BACKEND_TOKEN_EXCHANGE=/api/oauth/exchange

# Stable Diffusion / Image provider (if used client-side)
VITE_SD_API_URL=
VITE_SD_API_KEY=
